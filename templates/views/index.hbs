<div id="home">
	{{# each data.home.results}}
		<div class="content-container">
			<section id="slogan">
				{{{content.slogan}}}
				<img src="{{cloudinaryUrl content.sloganImage }}" alt="{{content.sloganImageAlt}}" />
			</section>
			
			<section id="introduction">
				{{{content.introduction}}}
			</section>
			
			<section id="paintingExample">
				<div id="imageRevealer" class="pic1">
					<div class="changeImage">View Original</div>
					<img src="/dist/imgs/original-photo.jpg" title="Oil | Before - Original Photo" width="1080" alt="Personalized wall art" height="598" class="original">
					<img src="/dist/imgs/painted-photo.jpg" title="Oil | After - Painted with Picarto" width="1080" height="598" alt="" class="new">
				</div>
				<div class="caption">
					{{{content.paintingExampleText}}}
				</div>
			</section>
			
			<section id="enticeExamples">
				<div class="textSection xlarge pink fadedMemories" style="background-image:url({{cloudinaryUrl fadedMemoriesImage }})">
					<img src="{{cloudinaryUrl fadedMemoriesImage }}" class="mobile">
					<div class="innerContent">
						{{{fadedMemoriesText}}}
					</div>
				</div>
				<div class="textSection xlarge pink lovedTogether" style="background-image:url({{cloudinaryUrl lovedOnesImage }})">
					<img src="{{cloudinaryUrl lovedOnesImage }}" class="mobile">
					<div class="innerContent">
						{{{lovedOnesText}}}
					</div>
				</div>
			</section>
		</div>
	
		<section class="largeImageWithText" id="married" style="background-image:url({{cloudinaryUrl marriedImage }});background-repeat:no-repeat;">
			<div class="content">
				{{{marriedText}}}
			</div>
		</section>
		
		
		<div class="content-container">
			<section id="how-does-it-work">
				{{{howDoesItWorkTitle}}}
				<div class="step">
					{{{howDoesStep1}}}
				</div>
				<div class="step">
					{{{howDoesStep2}}}
				</div>
				<div class="step">
					{{{howDoesStep3}}}
				</div>

			</section>
			
			<section id="last-minute">
				{{{lastMinute}}}
			</section>
		</div>
		
		<section class="largeImageWithText" id="madewithlove" style="background-image:url({{cloudinaryUrl madeWithLoveImage }});background-repeat:no-repeat;">
			<div class="content">
				{{{madeWithLoveText}}}
			</div>
		</section>
	{{/each}}
</div>